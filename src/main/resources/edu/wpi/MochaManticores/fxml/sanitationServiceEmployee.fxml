                           </children>
                        </GridPane>
                     </children>
                  </GridPane>
                  <JFXTextField fx:id="description" labelFloat="true" promptText="Description" GridPane.rowIndex="5" />
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="40.0" GridPane.rowIndex="6">
